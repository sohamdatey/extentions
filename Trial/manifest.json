{
    "name": "Action script injection demo",
    "version": "1.0",
    "manifest_version": 3,
    "content_security_policy": {
        "extension_pages": "default-src 'self'"
    },
    "action": {
        "default_title": "Click to show an alert"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "do.js"
    }
}